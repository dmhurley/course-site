{% extends 'BioPublicBundle::main.html.twig' %}

{% block head %}
	{{ parent() }}
	{% stylesheets 'bundles/biofolder/css/folder2.css' filter="cssrewrite" output="css/*.css" %}
		<link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}

	{% javascripts 'bundles/biopublic/js/Namer.js' %}
		<script src="{{asset_url}}"></script>
	{% endjavascripts %}
{% endblock %}

{% block content %}
<ul>
	{% include 'BioPublicBundle:Content:structure.html.twig' with {'root' : root, 'parent':true} %}
</ul>
{% if form is not null %}
	{{form(form)}}
{% endif %}
{% endblock %}