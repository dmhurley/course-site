{   
    "success": true,
    "message": "",
    "data": [
        {% for entity in entities %}
        {% if loop.index0 > 0 %},{% endif %}
        {
            "id": "{{entity.id}}",
            "title": "{{entity.title}}",
            "section": "{{entity.section}}",
            "tDate": "{{entity.tDate|date('m-d-y')}}",
            "tStart": "{{ entity.tStart|date('h:i') }}",
            "tEnd": "{{ entity.tEnd|date('h:i') }}",
            "tDuration": "{{ entity.tDuration }}",
            "gDate": "{{ entity.gDate|date('m-d-y') }}",
            "gStart": "{{ entity.gStart|date('h:i') }}",
            "gEnd": "{{ entity.gEnd|date('h:i') }}",
            "gDuration": "{{ entity.gDuration }}",
            "questions": "{{entity.questions|length }}"
        }
        {% endfor %}
    ]
}