{% extends 'BioStyleBundle:Default:base.html.twig' %}
{% block title %}UW - Biology{% endblock %}

{% block head %}
	{% block stylesheets %}
		{% stylesheets 'bundles/biostyle/css/page.css' filter="cssrewrite" output="css/*.css" %}
			<link rel="stylesheet" href="{{ asset_url }}" />
		{% endstylesheets %}
	{% endblock %}
{% endblock %}

{% block body %}
	<div id="container">
		<div id="page">
			<div id="header">
				<a id="logo" href="http://www.biology.washington.edu"></a>
				<div id="dub">
					<div id="wLogo">
						<div id="w">
							<a href="http://www.washington.edu"></a>
						</div>
						<!-- <div id="x">
							<a href="http://www.washington.edu/"></a>
						</div> -->
					</div>
				</div>
			</div>

			<div id="left">
				<div class="content">
					<ul class="menu">
						<li>
							<a href="{{ path('display_students') }}">Students</a>
							<ul>
								<li><a href="{{ path('display_students') }}">Display</a></li>
								<li><a href="{{ path('find_student') }}">Find</a></li>
								<li><a href="{{ path('add_student') }}">Add</a></li>
								<li><a href="{{ path('upload_student') }}">Upload</a></li>
							</ul>
						</li>
						<li><a href="">Clickers</a></li>
					</ul>
				</div>
			</div>

			<div id="center">
				{% if title is defined %}
					<h1>{{title}}</h1>
				{% endif %}
				<div id="content">
					{% block content %}

					{% endblock %}
				</div>
			</div>
		</div>

		<div id="footer">

		</div>
	</div>
{% endblock %}